[*]
charset=utf-8
end_of_line=lf
trim_trailing_whitespace=true
insert_final_newline=true
indent_style=space
indent_size=4

[{*.ng,*.sht,*.html,*.shtm,*.shtml,*.htm}]
indent_style=tab
tab_width=4

[*.cjs]
indent_style=tab
tab_width=4

[{*.es6,*.mjs}]
indent_style=tab
tab_width=4

[*.vue]
indent_style=tab
tab_width=4

[{.analysis_options,*.yml,*.yaml}]
indent_style=space
indent_size=4

